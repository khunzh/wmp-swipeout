.wm-swipeout-item {
  position: relative;
  width: 100%;
  overflow: hidden;
}

.wm-swipeout-item-right {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  font-size: 0;
  text-align: right;
}

.wm-swipeout-item-right .right-wrap {
  display: inline-block;
  height: 100%;
}

.mp-swipeout-item-content {
  position: relative;
  background: #fff;
}

.wm-swipeout-item-right.animated, .mp-swipeout-item-content.animated{
   transition: transform 0.2s;
}